CREATE DOMAIN COUCH_ROWS AS REAL CHECK
(value >= 1 AND value <= 7);

CREATE DOMAIN ROW_SEAT AS CHAR CHECK
(value = 'A' OR value = 'B' OR value = 'C' OR value = 'D' OR value = 'E' OR value = 'F' OR value = 'G');

CREATE TABLE bookings
(
ROW_NO COUCH_ROWS,
SEAT_NO ROW_SEAT,
AVAILABLE BOOlean NOT NULL,   
PRIMARY KEY (ROW_NO,SEAT_NO)
);

